#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/script/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/script/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/cmd" ]; then
  exec "$basedir/cmd" /C "$basedir/../.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/script/vscode.cmd" "$@"
else
  exec cmd /C "$basedir/../.pnpm/@electron-forge+cli@7.8.2_encoding@0.1.13/node_modules/@electron-forge/cli/script/vscode.cmd" "$@"
fi
