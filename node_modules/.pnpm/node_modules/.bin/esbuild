#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/bin/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/bin/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/esbuild@0.25.8/node_modules:/home/dev-shabbir/Downloads/ledger-live-desktop-win32-x64-1.0.0/ledger-live-desktop-win32-x64/resources/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
